#include<stdio.h>
#include<conio.h>

 void search(int a[],int n, int item)

{
    int value=1,i;

    for(i=0;i<=n-1;i++)
    {
        if(a[i]==item)
        {


          value=0;
          printf("successful");

        }

    }



    if(value==1)
    printf("search unsuccessfull");

}

void main()
{
    int a[]={45,3,6,8,9,12,45,67};

    search(a,8,12);
}
